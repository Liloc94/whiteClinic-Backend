{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api-docs",
      "dest": "/src/main.ts"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "/src/main.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/main.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/main.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/main.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
