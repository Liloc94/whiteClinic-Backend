{
  "version": 2,
  "builds": [
    {
      "src": "backend/dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "/backend/dist/public/api-docs/$1",
      "headers": {
        "cache-control": "no-store"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/backend/dist/main.js"
    }
  ]
}
